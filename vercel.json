{
  "version": 2,
  "buildCommand": "CI=false GENERATE_SOURCEMAP=false NODE_OPTIONS='--max-old-space-size=8192' npm run build",
  "outputDirectory": "build",
  "framework": "create-react-app",
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/favicon.ico",
      "dest": "/favicon.ico"
    },
    {
      "src": "/logo192.png",
      "dest": "/logo192.png"
    },
    {
      "src": "/logo512.png",
      "dest": "/logo512.png"
    },
    {
      "src": "/manifest.json",
      "dest": "/manifest.json"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "REACT_APP_MAP_CENTER_LAT": "20.5937",
    "REACT_APP_MAP_CENTER_LNG": "78.9629",
    "REACT_APP_MAP_ZOOM": "5",
    "NODE_OPTIONS": "--max-old-space-size=8192"
  }
} 